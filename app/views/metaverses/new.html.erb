
<div class="container" >
  <h1>Create your new Metaverse, change the world</h1>
  <br>
  <%= simple_form_for(@metaverse) do |f| %>
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :price %>
      <%= f.input :pictures, as: :file, input_html: { multiple: true } %>
      <%= f.submit %>
  <% end %>
  <br>
  <div>
    <%= link_to "Back to list", metaverses_path, class: "btn btn-primary" %>
  </div>
</div>
